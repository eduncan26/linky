{
  "name": "linky",
  "version": "1.0.0",
  "description": "Linked lists for JS, and other data structures that build on them.",
  "keywords": [
    "linked",
    "list",
    "linked list",
    "stack",
    "queue",
    "data",
    "structure",
    "data structure"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src -d lib --ignore *.spec.js",
    "lint": "eslint src/**/*.js",
    "test": "mocha src/**/*.spec.js --require babel-polyfill --require babel-register",
    "test:coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:all": "testen"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/evan-duncan/linky.git"
  },
  "author": "Evan Duncan <evan.e.duncan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evan-duncan/linky/issues"
  },
  "homepage": "https://github.com/evan-duncan/linky#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-mocha": "^5.0.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "testen": "^2.2.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "testen": {
    "node": [
      "4",
      "5",
      "6",
      "7",
      "8",
      "9",
      "10"
    ]
  },
  "babel": {
    "plugins": ["transform-runtime"],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 0.5%",
              "last 2 versions",
              "Firefox ESR",
              "not dead"
            ],
            "node": 4
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "plugins": [
      "mocha"
    ],
    "env": {
      "mocha": true,
      "node": true,
      "browser": true
    }
  }
}
